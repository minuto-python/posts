   "source": [
    "# Criando a função que será aplicada \n",
    "# a cada item da lista\n",
    "\n",
    "def calc(item):\n",
    "    result = item * 2\n",
    "    print(f\"({item}) x 2 = {result}\")\n",
    "    return result"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "0cca6c96",
   "metadata": {},
   "outputs": [
